<!DOCTYPE html>
<html>
<body>

<input type="text" id="inputField" placeholder="Enter something...">
<button onclick="sendRequest()">Send</button>

<script>
function sendRequest() {
  var input = document.getElementById('inputField').value;
  var userID = '<%= userID %>'; // replace with your userID
  var uniqueString = '<%= uniqueString %>'; // replace with your uniqueString

  fetch(`http://localhost:3002/user/changePassword/${userID}/${uniqueString}/setPassword`, { // replace with your endpoint
    method: 'POST',
    headers: {
      'Content-Type': 'application/json',
    },
    body: JSON.stringify({ password: input, userID: userID, uniqueString: uniqueString }),
  })
  .then(response => response.json())
  .then(data => console.log(data))
  .catch((error) => {
    console.error('Error:', error);
  });
}
</script>

</body>
</html>